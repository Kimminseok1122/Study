CREATE TABLE TEST(
	TEST_ID INT PRIMARY KEY AUTO_INCREMENT,
    TEST_REGDATE TIMESTAMP DEFAULT NOW(),
    TEST_ENDDATE VARCHAR(20),
    TEST_WRITER VARCHAR(20),
    TEST_PRICE INT NOT NULL,
    TEST_COUNT INT NOT NULL,
    TEST_DISCOUNT INT DEFAULT 0,
    TEST_PROD_PURCHASE_YN VARCHAR(1) DEFAULT 'N' CHECK(TEST_PROD_PURCHASE_YN IN ('Y','N')),
    TEST_CONTENT VARCHAR(1000),
    TEST_COMMENT VARCHAR(200)
);